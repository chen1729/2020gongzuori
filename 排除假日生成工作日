    if($yue<=9){
        $yue='0'.$yue;
    }
    if(($yue==1)or ($yue==3) or ($yue==5) or ($yue==7) or ($yue==8) or ($yue==10) or ($yue==12)){
        $ri1='31';
    }elseif($yue==2){
        $ri1='29';
    }else{
        $ri1='30';
    }


    for($ri=1;$ri<=$ri1;$ri++){
        if($ri<=9){
            $ri='0'.$ri;
